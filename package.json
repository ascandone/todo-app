{
  "name": "react-starter-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "test:watch": "vitest --ui",
    "test:run": "vitest --run --coverage",
    "storybook": "storybook dev -p 6006 -h localhost",
    "build-storybook": "storybook build",
    "cypress": "cypress open --e2e",
    "cypress:run": "wait-on http://localhost:3000 && cypress run",
    "ts:check": "tsc",
    "ts:watch": "tsc --watch",
    "format:check": "prettier --check --cache .",
    "format:write": "prettier --write --cache .",
    "lint:check": "eslint ./src --cache",
    "lint:write": "eslint ./src --fix"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "@prisma/client": "4.6.0",
    "@tanstack/react-query": "^4.14.5",
    "@trpc/client": "^10.0.0-rc.5",
    "@trpc/react-query": "^10.0.0-rc.5",
    "@trpc/server": "^10.0.0-rc.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@storybook/addon-actions": "^7.0.0-alpha.48",
    "@storybook/addon-essentials": "^7.0.0-alpha.48",
    "@storybook/addon-interactions": "^7.0.0-alpha.48",
    "@storybook/addon-links": "^7.0.0-alpha.48",
    "@storybook/react": "^7.0.0-alpha.48",
    "@storybook/react-vite": "^7.0.0-alpha.48",
    "@storybook/testing-library": "^0.0.14-next.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-istanbul": "^0.25.1",
    "@vitest/ui": "^0.25.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "cypress": "^10.11.0",
    "eslint": "^8.27.0",
    "eslint-plugin-react": "^7.31.10",
    "jsdom": "^20.0.2",
    "next": "^13.0.2",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prisma": "^4.6.0",
    "storybook": "^7.0.0-alpha.48",
    "tailwindcss": "^3.2.2",
    "typescript": "^4.8.4",
    "vercel": "^28.4.17",
    "vite": "^3.2.3",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.1",
    "wait-on": "^6.0.1"
  }
}
